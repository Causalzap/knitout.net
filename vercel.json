        }
      ]
    }
  ],
  "redirects": [
    { "source": "/games/color-block-jam", "destination": "/color-block-jam/", "permanent": true },
    { "source": "/games/color-block-jam/", "destination": "/color-block-jam/", "permanent": true },
    { "source": "/games/color-block-jam/index.html", "destination": "/color-block-jam/", "permanent": true },

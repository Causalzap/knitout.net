    { "source": "/games/sand-blast-block-game", "destination": "/sand-blast-block-game/", "permanent": true },
    { "source": "/games/sand-blast-block-game/", "destination": "/sand-blast-block-game/", "permanent": true },
    { "source": "/games/sand-blast-block-game/index.html", "destination": "/sand-blast-block-game/", "permanent": true },
    { "source": "/sand-blast-block-game/index.html", "destination": "/sand-blast-block-game/", "permanent": true },

    { "source": "/games/stickhole-io", "destination": "/stickhole-io/", "permanent": true },
    { "source": "/games/stickhole-io/", "destination": "/stickhole-io/", "permanent": true },
    { "source": "/games/stickhole-io/index.html", "destination": "/stickhole-io/", "permanent": true },
    { "source": "/stickhole-io/index.html", "destination": "/stickhole-io/", "permanent": true },

    { "source": "/index.html", "destination": "/", "permanent": true }
],
"rewrites": [
  { "source": "/color-block-jam/", "destination": "/games/color-block-jam/index.html" },
  { "source": "/sand-blast-block-game/", "destination": "/games/sand-blast-block-game/index.html" },
  { "source": "/stickhole-io/", "destination": "/games/stickhole-io/index.html" }
]
  
}

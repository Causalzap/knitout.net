  <script is:inline>
    (function () {
      function closeAll(exceptId) {
        document.querySelectorAll('[data-faq-toggle]').forEach(function (btn) {
          var id = btn.getAttribute('aria-controls');
          if (!id || id === exceptId) return;

          var panel = document.getElementById(id);
          btn.setAttribute('aria-expanded', 'false');

          var item = btn.closest('.sh-faq-item');
          if (item) item.setAttribute('data-open', 'false');

          if (panel) panel.style.maxHeight = '0px';
        });
      }

      function init() {
        var buttons = document.querySelectorAll('[data-faq-toggle]');
        if (!buttons.length) return;

        buttons.forEach(function (btn) {
          btn.addEventListener('click', function () {
            var id = btn.getAttribute('aria-controls');
            if (!id) return;

            var panel = document.getElementById(id);
            if (!panel) return;

            var expanded = btn.getAttribute('aria-expanded') === 'true';
            if (!expanded) closeAll(id);

            btn.setAttribute('aria-expanded', expanded ? 'false' : 'true');

            var item = btn.closest('.sh-faq-item');
            if (item) item.setAttribute('data-open', expanded ? 'false' : 'true');

            if (expanded) panel.style.maxHeight = '0px';
            else panel.style.maxHeight = panel.scrollHeight + 'px';
          });
        });
      }

      document.addEventListener('DOMContentLoaded', init);
    })();
  </script>
